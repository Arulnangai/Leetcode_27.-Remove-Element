class Solution {
    public int removeElement(int[] nums, int val) {
        int insert_position = 0;
        for(int i = 0 ; i <nums.length;i++)
        {
            if(nums[i]!=val)
            {
                nums[insert_position] = nums[i];
                insert_position++;
            }
        }
         return insert_position;
      
       

        
    }
}
